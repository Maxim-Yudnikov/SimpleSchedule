<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.SimpleSchedule" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/dark_color_primary</item>
    </style>

    <style name="Background">
        <item name="android:background">@color/dark_background</item>
    </style>

    <style name="RecyclerView">
        <item name="android:background">@color/dark_recycler_view</item>
    </style>

    <style name="DayLayout">
        <item name="android:background">@color/dark_day_layout</item>
    </style>

    <style name="LessonLayout">
        <item name="android:background">@color/dark_lesson_layout</item>
    </style>

    <style name="LessonTimeLayout">
        <item name="android:background">@color/dark_lesson_layout</item>
        <item name="android:textColor">@color/dark_time</item>
    </style>

    <style name="Button">
        <item name="android:backgroundTint">@color/dark_button</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>